<library path="../../../../devel/lib/libtrajectory_trackers_plugin">
  <!-- Standard Nested Position - Attitude Controller -->
  <class type="trajectory_trackers_plugin::StandardTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>High level Position Controller with (nested) attitude controller</description>
  </class>
  <!-- Nested Position + external force - Attitude Controller -->
  <class type="trajectory_trackers_plugin::PositionForceTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>High level Position + external force compensation Controller with (nested) attitude controller</description>
  </class>
  <!-- Euroc -->
  <class type="trajectory_trackers_plugin::EurocTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>High level Position Controller with (nested) attitude controller</description>
  </class>
  <!-- SMURF Controller -->
  <class type="trajectory_trackers_plugin::SMURFTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Trajectory controller in SE3</description>
  </class>
  <!-- MK_OMEGA Controller -->
  <class type="trajectory_trackers_plugin::OmegaNHFCTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Near Hovering flight controller in high level code</description>
  </class>
  <!-- Adaptive Controller -->
  <class type="trajectory_trackers_plugin::AdaptiveTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Adaptive controller</description>
  </class>
  <!-- Teleoperation Controller that will overwrite some /all of the commanded values for the mkinterface -->
  <class type="trajectory_trackers_plugin::TeleoperationTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Teleoperation controller</description>
  </class>
  <!-- yuyi -->
 <!-- Global-Output-Regulation-based Controller -->
  <class type="trajectory_trackers_plugin::GeometricTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Global-Output-Regulation-based controller</description>
  </class>
  <!-- Tube-based Model Predictive Controller -->
  <class type="trajectory_trackers_plugin::TubeMPCTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Tube-based Model Predictive controller</description>
  </class>
  <!-- Robust Model Predictive Controller -->
  <class type="trajectory_trackers_plugin::RoMPCTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Robust Model Predictive controller for TRO paper</description>
  </class>
  <!-- Robust Backstepping Controller -->
  <class type="trajectory_trackers_plugin::RoBSCTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Robust Backstepping controller for TRO paper</description>
  </class>
</library>
