<package>
  <name>telekyb_wittensteinreceiver</name>
  <version>1.0.0</version>
  <description>telekyb_wittensteinreceiver receives the inputs given by the wittensteins and sent by the xPC/Simulink model</description>
  <maintainer email="eugen.ruff@tuebingen.mpg.de">Eugen Ruff</maintainer>

  <license>BSD</license>

  <author>Johannes Laechele</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>roscpp</build_depend>
  <build_depend>telekyb_defines</build_depend>
  <build_depend>telekyb_base</build_depend>
  <build_depend>sensor_msgs</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>roscpp</run_depend>
  <run_depend>telekyb_defines</run_depend>
  <run_depend>telekyb_base</run_depend>
  <run_depend>sensor_msgs</run_depend>

</package>
