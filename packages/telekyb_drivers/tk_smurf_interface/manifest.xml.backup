<package>
  <description brief="tk_smurf_interface">

     tk_smurf_interface reads IMU Data from serial in specific format and publishes it as sensor_msgs/IMU
     if desired it also listen to a command topic and sends commands to the BL-controllers

  </description>
  <author>Martin Riedel</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/tk_smurf_interface</url>
  <depend package="roscpp"/>
  <depend package="tk_draft_msgs"/>
  <depend package="telekyb_msgs"/>
  <depend package="telekyb_base"/>
  <depend package="telekyb_serial"/>

</package>


