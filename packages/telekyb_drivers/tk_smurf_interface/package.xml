<package>
  <name>tk_smurf_interface</name>
  <version>3.0.0</version>
  <description>tk_smurf_interface reads IMU Data from serial in specific format and publishes it as sensor_msgs/IMU
     if desired it also listen to a command topic and sends commands to the BL-controllers</description>
  <maintainer email="eugen.ruff@tuebingen.mpg.de">Eugen Ruff</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/tk_smurf_interface</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Martin Riedel</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>roscpp</build_depend>
  <build_depend>tk_draft_msgs</build_depend>
  <build_depend>telekyb_msgs</build_depend>
  <build_depend>telekyb_base</build_depend>
  <build_depend>telekyb_serial</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>roscpp</run_depend>
  <run_depend>tk_draft_msgs</run_depend>
  <run_depend>telekyb_msgs</run_depend>
  <run_depend>telekyb_base</run_depend>
  <run_depend>telekyb_serial</run_depend>

  <!-- Dependencies needed only for running tests. -->
  <!-- <test_depend>roscpp</test_depend> -->
  <!-- <test_depend>tk_draft_msgs</test_depend> -->
  <!-- <test_depend>telekyb_msgs</test_depend> -->
  <!-- <test_depend>telekyb_base</test_depend> -->
  <!-- <test_depend>telekyb_serial</test_depend> -->

</package>
