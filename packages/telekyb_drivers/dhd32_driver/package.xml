<package>
  <name>dhd32_driver</name>
  <version>3.0.0</version>
  <description>dhd32_driver is a driver for the for products for Force Dimensions (linked against DHD3.2). Logic
     is provided by dynamically loaded HapticDeviceControllers</description>
  <maintainer email="eugen.ruff@tuebingen.mpg.de">Eugen Ruff</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/dhd32_driver</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Martin Riedel</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>roscpp</build_depend>
  <build_depend>dhd32_wrapper</build_depend>
  <build_depend>telekyb_base</build_depend>
  <build_depend>telekyb_defines</build_depend>
  <build_depend>tk_haptics_base</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>roscpp</run_depend>
  <run_depend>dhd32_wrapper</run_depend>
  <run_depend>telekyb_base</run_depend>
  <run_depend>telekyb_defines</run_depend>
  <run_depend>tk_haptics_base</run_depend>

  <!-- Dependencies needed only for running tests. -->
  <!-- <test_depend>roscpp</test_depend> -->
  <!-- <test_depend>dhd32_wrapper</test_depend> -->
  <!-- <test_depend>telekyb_base</test_depend> -->
  <!-- <test_depend>tk_haptics_base</test_depend> -->

</package>
