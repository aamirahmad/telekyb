<package>
  <name>dhd_driver</name>
  <version>3.0.0</version>
  <description>dhd_driver is a driver for the for products for Force Dimensions (linked against latest release of DHD).</description>
  <maintainer email="eugen.ruff@tuebingen.mpg.de">Eugen Ruff</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/dhd_driver</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Dr. Antonio Franchi and Martin Riedel</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>roscpp</build_depend>
  <build_depend>telekyb_defines</build_depend>
  <build_depend>telekyb_msgs</build_depend>
  <build_depend>telekyb_base</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>dhd_wrapper</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>roscpp</run_depend>
  <run_depend>telekyb_defines</run_depend>
  <run_depend>telekyb_msgs</run_depend>
  <run_depend>telekyb_base</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>dhd_wrapper</run_depend>

  <!-- Dependencies needed only for running tests. -->
  <!-- <test_depend>roscpp</test_depend> -->
  <!-- <test_depend>telekyb_defines</test_depend> -->
  <!-- <test_depend>telekyb_msgs</test_depend> -->
  <!-- <test_depend>tf</test_depend> -->
  <!-- <test_depend>dhd_wrapper</test_depend> -->

</package>
