<library path="lib/libtrajectory_trackers">
  <!-- Standard Nested Position - Attitude Controller -->
  <class name="tk_trajctrl/StandardTrajectoryTracker" type="telekyb::StandardTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>High level Position Controller with (nested) attitude controller</description>
  </class>
  <!-- SMURF Controller -->
  <class name="tk_trajctrl/SMURFTrajectoryTracker" type="telekyb::SMURFTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Trajectory controller in SE3</description>
  </class>
  <!-- Adaptive Controller -->
  <class name="tk_trajctrl/AdaptiveTrajectoryTracker" type="telekyb::AdaptiveTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Adaptive controller</description>
  </class>
  <!-- Global-Output-Regulation-based Controller -->
  <class name="tk_trajctrl/GeometricTrajectoryTracker" type="telekyb::GeometricTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Global-Output-Regulation-based controller</description>
  </class>
  <!-- Tube-based Model Predictive Controller -->
  <class name="tk_trajctrl/TubeMPCTrajectoryTracker" type="telekyb::TubeMPCTrajectoryTracker" base_class_type="telekyb::TrajectoryTracker">
    <description>Tube-based Model Predictive controller</description>
  </class>

</library>
